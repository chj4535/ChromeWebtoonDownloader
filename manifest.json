{
  "manifest_version": 2,
  "name": "ChromeWebtoonDownloader",
  "version": "0.2",
  "browser_action": {
    "default_icon": "icon_able.png",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"]
}